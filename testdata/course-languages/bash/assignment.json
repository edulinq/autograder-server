{
    "id": "bash",
    "name": "A Simple Bash Assignment",
    "due-date": 0,
    "static-files": [
        "grader.sh"
    ],
    "image": "ghcr.io/edulinq/grader.base:0.1.1-alpine",
    "invocation": ["bash", "./grader.sh"],
    "post-submission-file-ops": [
        ["cp", "input/assignment.sh", "work/assignment.sh"]
    ],
    "analysis-options": {
        "include-patterns": [
            "assignment.sh"
        ],
        "template-files": [
            "test-submissions/not-implemented/assignment.sh"
        ]
    }
}
